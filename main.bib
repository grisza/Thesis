	journal = {In Pract.},
	mendeley-groups = {Filters},
	number = {1},
	pages = {1--16},
	pmid = {20578276},
	title = {{An Introduction to the Kalman Filter}},
	url = {http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.79.6578{\&}amp;rep=rep1{\&}amp;type=pdf},
	volume = {7},
	year = {2006}
