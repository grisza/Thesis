	author = {Konatowski, Stanisław},
	file = {:C$\backslash$:/Users/grzeg/Documents/Mendeley Desktop/2007/Biuletyn Wojskowej Akademii Technicznej/2007 - Konatowski - Dokładno{\'{s}}{\'{c}} procesu estymacji w wybranych filtrach Kalmana.pdf:pdf},
