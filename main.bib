	keywords = {Extended Kalman Filter,Unscented Kalman Filter,estimation,nonlinear filtration,particle filter},
	number = {2},
	pages = {243--253},
	title = {{Ocena dokładno{\'{s}}ci filtracji nieliniowej w procesie okre{\'{s}}lania położenia}},
	volume = {56},
