@article{Ali2001,
	abstract = {This paper presents a methodology for automatic segmentation and recognition of continuous human activity. We segment a continuous human activity into separate actions and correctly identify each action. The camera views the subject from the lateral view. There are no distinct breaks or pauses between the execution of different actions. We have no prior knowledge about the commencement or termination of each action. We compute the angles subtended by three major components of the body with the vertical axis, namely the torso, the upper component of the leg and the lower component of the leg. Using these three angles as a feature vector, we classify frames into breakpoint and non-breakpoint frames. Breakpoints indicate an action's commencement or termination. We use single action sequences for the training data set. The test sequences, on the other hand, are continuous sequences of human activity that consist of three or more actions in succession. The system has been tested on continuous activity sequences containing actions such as walking, sitting down, standing up, bending, getting up, squatting and rising. It detects the breakpoints and classifies the actions between them.},
	author = {Ali, A. and Aggarwal, J. K.},
	doi = {10.1109/EVENT.2001.938863},
	file = {:C$\backslash$:/Users/GLONEKG/Downloads/00938863.pdf:pdf},
	isbn = {0769512933},
	journal = {Proceedings - IEEE Workshop on Detection and Recognition of Events in Video, EVENT 2001},
	mendeley-groups = {Thesis},
	pages = {28--35},
	title = {{Segmentation and recognition of continuous human activity}},
	year = {2001}
}


@article{Veenman2001,
	abstract = {Studies the motion correspondence problem for which a diversity of$\backslash$nqualitative and statistical solutions exist. We concentrate on$\backslash$nqualitative modeling, especially in situations where assignment$\backslash$nconflicts arise either because multiple features compete for one$\backslash$ndetected point or because multiple detected points fit a single feature$\backslash$npoint. We leave out the possibility of point track initiation and$\backslash$ntermination because that principally conflicts with allowing for$\backslash$ntemporary point occlusion. We introduce individual, combined, and global$\backslash$nmotion models and fit existing qualitative solutions in this framework.$\backslash$nAdditionally, we present a tracking algorithm that satisfies$\backslash$nthese-possibly constrained-models in a greedy matching sense, including$\backslash$nan effective way to handle detection errors and occlusion. The$\backslash$nperformance evaluation shows that the proposed algorithm outperforms$\backslash$nexisting greedy matching algorithms. Finally, we describe an extension$\backslash$nto the tracker that enables automatic initialization of the point$\backslash$ntracks. Several experiments show that the extended algorithm is$\backslash$nefficient, hardly sensitive to its few parameters, and qualitatively$\backslash$nbetter than other algorithms, including the presumed optimal statistical$\backslash$nmultiple hypothesis tracker},
