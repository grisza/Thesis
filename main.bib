	journal = {J. Basic Eng.},
	keywords = {kalman filter,lineal filter,prediction,predictor},
	mendeley-groups = {Thesis},
	number = {1},
	pages = {35},
	pmid = {5311910},
	title = {{A New Approach to Linear Filtering and Prediction Problems}},
	url = {http://scholar.google.com/scholar?hl=en{\&}btnG=Search{\&}q=intitle:A+New+Approach+to+Linear+Filtering+and+Prediction+Problems{\#}0 http://fluidsengineering.asmedigitalcollection.asme.org/article.aspx?articleid=1430402},
